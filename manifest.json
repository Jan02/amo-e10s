{
	"manifest_version": 2,
	"name": "amo e10s",
	"version": "1",

	"description": "display e10s compatibility on amo",
	"icons": {
		"48": "icon48.png",
		"96": "icon96.png"
	},
	"applications": {
		"gecko": {
			"id": "amo-e10s@jetpack",
			"strict_min_version": "45.0"
		}
	},

	"content_scripts": [{
		"matches": ["https://addons.mozilla.org/*/firefox/addon/*/*"],
		"js": ["jszip.min.js", "watchify.js"]
	}],

	"permissions": [
		"https://addons.cdn.mozilla.net/*"
	]

}
